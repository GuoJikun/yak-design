{
  "name": "yak-ui",
  "version": "1.0.0",
  "main": "dist/yak-ui.umd.js",
  "module": "dist/yak-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/yak-ui.es.js",
      "require": "./dist/yak-ui.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lib:dev": "rollup -c rollup.config.dev.js -w",
    "lib:build": "rollup -c rollup.config.prod.js"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.1",
    "async-validator": "^4.0.3",
    "dayjs": "^1.10.6",
    "rollup": "^2.61.1",
    "vue": "^3.2.6"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vue/compiler-sfc": "^3.2.6",
    "node-sass": "^6.0.1",
    "postcss-import": "^14.0.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.4",
    "vue-tsc": "^0.2.2"
  }
}
